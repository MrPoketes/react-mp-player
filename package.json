{
	"name": "react-mp-player",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@types/node": "^16.7.13",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^18.0.0",
		"react": "^17.0.2",
		"react-dom": "^18.1.0",
		"react-scripts": "5.0.1",
		"typescript": "^4.4.2",
		"web-vitals": "^2.1.0"
	},
	"scripts": {
		"start": "concurrently -k \"yarn build:tailwind --watch\"  \"start-storybook -p 6006\"",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"build:storybook": "build-storybook",
		"build:tailwind": "tailwind -i ./.storybook/index.css -o ./.storybook/index.generated.css"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@storybook/addon-actions": "^6.5.8",
		"@storybook/addon-essentials": "^6.5.8",
		"@storybook/addon-interactions": "^6.5.8",
		"@storybook/addon-links": "^6.5.8",
		"@storybook/builder-webpack5": "^6.5.8",
		"@storybook/manager-webpack5": "^6.5.8",
		"@storybook/node-logger": "^6.5.8",
		"@storybook/preset-create-react-app": "^4.1.2",
		"@storybook/react": "^6.5.8",
		"@storybook/testing-library": "^0.0.12",
		"@vitejs/plugin-react": "^1.3.2",
		"autoprefixer": "^10.4.7",
		"babel-plugin-named-exports-order": "^0.0.2",
		"concurrently": "^7.2.1",
		"postcss": "^8.4.14",
		"prop-types": "^15.8.1",
		"tailwindcss": "^3.1.2",
		"vite": "^2.9.12",
		"vite-dts": "^1.0.4",
		"vite-plugin-dts": "^1.2.0",
		"webpack": "^5.73.0"
	}
}
